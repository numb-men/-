  width: 400rpx;
  padding-top: 6rpx;
  padding-left: 8rpx;
  font-weight: bold;
}
.human-phone {
  font-size: 23rpx;
  color: #ffcc00;
  padding-top: 6rpx;
  line-height: 50rpx;
  text-align: right;
  width: 100rpx; /*150*/
  padding-right: 60rpx;
}
.position-item-center {
  padding-bottom: 15rpx;
}
.position-province {
  font-size: 23rpx;
