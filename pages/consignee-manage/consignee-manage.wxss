.order-list {
  padding: 25rpx 15rpx;
  background: #f6f4f7;
}
.order-item-contain {
  padding: 0 10rpx;
}
.left-style {
  width: 30rpx;
  height: 220rpx;
}
.left-style-ends {
  width: 6rpx;
  height: 95rpx;
  margin: 0 12rpx;
  background: #fefefe;
}
.left-style-center {
  width: 14rpx;
  height: 14rpx;
  margin: 0 8rpx;
  border-radius: 50%;
  background: #fefefe;
}
.position-item {
  width: 600rpx;
  height: 200rpx;
